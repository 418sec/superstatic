  },
  "dependencies": {
    "as-array": "^0.1.2",
    "async": "~0.9.0",
    "cache-control": "^0.1.0",
    "chalk": "~0.4.0",
    "chokidar": "~0.8.0",
    "compression": "~1.0.2",
    "connect": "3.x",
    "connect-query": "~0.1.0",
    "deliver": "~0.2.0",
    "drainer": "~0.2.1",
    "fast-url-parser": "^1.0.6-0",
    "feedback": "~0.3.2",
    "globject": "0.1.x",
    "is-url": "~1.1.0",
    "lodash": "2.4.x",
    "mime-types": "^1.0.0",
    "minimatch": "0.3.x",
