    "stacked": "^1.1.0",
    "superstatic-redirects": "^0.1.0",
    "superstatic-routes": "^0.1.0",
    "update-notifier": "~0.2.0",
    "van": "0.0.4"
  },
  "devDependencies": {
    "chai": "~1.9.1",
    "clone": "0.1.x",
    "connect": "3.x",
    "jshint": "2.5.x",
    "knox": "~0.8.6",
    "mkdirp": "0.5.x",
    "mocha": "~1.19.0",
    "mocksy": "~0.1.4",
    "request": "~2.36.0",
    "rmdir": "1.0.x",
    "serve-static": "~1.1.0",
    "sinon": "~1.10.0",
    "supertest": "^0.13.0",
    "args-list": "^0.3.2"
  },
  "jshintConfig": {
    "expr": true,
    "curly": false,
    "eqeqeq": true,
    "immed": true,
    "latedef": false,
    "newcap": true,
    "noarg": true,
    "sub": true,
    "undef": true,
    "unused": false,
    "boss": true,
    "eqnull": true,
    "node": true,
    "laxbreak": true,
    "globals": {
      "require": true,
      "module": true,
      "exports": true,
      "it": true,
      "describe": true,
      "beforeEach": true,
      "afterEach": true,
      "chai": true,
      "Regex": true
    }
