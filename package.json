{
  "name": "superstatic",
  "version": "0.12.11",
  "description": "Superstatic: a static file server for fancy apps",
  "main": "lib",
  "scripts": {
    "test": "npm run lint && mocha ./test",
    "lint": "jshint ./lib",
    "bench": "wrk -t1 -c400 -d15s http://localhost:3474"
  },
  "bin": {
    "superstatic": "bin/server",
    "ss": "bin/server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/divshot/superstatic.git"
  },
  "keywords": [
    "static",
    "server",
    "divshot",
    "hosting"
